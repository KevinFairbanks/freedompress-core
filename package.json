{
  "name": "@freedompress/core",
  "version": "1.0.0",
  "description": "Core framework for FreedomPress - Modern CMS with modular architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "freedompress": "dist/cli/index.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:lib",
    "build:lib": "tsc --project tsconfig.build.json",
    "build:cli": "tsc src/cli/index.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --resolveJsonModule --skipLibCheck --moduleResolution node",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "prepublishOnly": "npm run build:lib && npm run build:cli",
    "prepack": "npm run build:lib"
  },
  "keywords": [
    "freedompress",
    "cms",
    "nextjs",
    "react",
    "typescript",
    "modular",
    "framework",
    "blog",
    "ecommerce"
  ],
  "author": "FreedomPress Team",
  "license": "MIT",
  "homepage": "https://github.com/KevinFairbanks/freedompress-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinFairbanks/freedompress-core.git"
  },
  "bugs": {
    "url": "https://github.com/KevinFairbanks/freedompress-core/issues"
  },
  "files": [
    "dist",
    "prisma",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.11.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/bcryptjs": "^3.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "crypto-js": "^4.2.0",
    "express-rate-limit": "^7.5.1",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "inquirer": "^8.2.6",
    "jest": "^30.0.4",
    "joi": "^17.13.3",
    "next": "^15.3.5",
    "next-auth": "^4.24.11",
    "node-mocks-http": "^1.17.2",
    "ora": "^5.4.1",
    "postcss": "^8.5.6",
    "prisma": "^6.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^5.6.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/react-dom": "^19.1.6",
    "ts-jest": "^29.4.0"
  }
}
